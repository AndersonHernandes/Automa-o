<script>
    function pop_it_up_center(page_URL, title, w, h) {
        let left  = (screen.width / 2) - (w / 2);
        let top   = (screen.height / 2) - (h / 2);
        let popup = window.open(page_URL, title, 'width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
        if (window.focus) {
            popup.focus();
        }
    }

    function carregar_mapa_20(franquia) {
        var verifyMbox18 = setInterval(() => {
            if ($('#mbox18').length >= 1) {
                clearInterval(verifyMbox18);
                $('#conteudo_item_20').html('<img src="./images/carregando_pacotes.gif" width="32" height="32" align="absmiddle" />');
                $('#conteudo_item_20').load('./ajax/mapa_trabalho_item_20.php', {franquia: franquia}, function() {
                    $('#bprosseguir').show();
                });
            }
        }, 1000);
    }

    function mostrarMaisinhos() {
        $('.maisinho').show();
    }

    $(document).ready(function() {
        mostrarMaisinhos();
        carregar_mapa_20(1693);
    });
</script>
