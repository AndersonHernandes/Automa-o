<script>
	$(document).ready(function () {
		$("#open_mapa_admin").off("click").on("click", function () {
			deny_ver_dados();
			pop_it_up_center("../../admin/mapa_trabalho_colaborador_link.php", "janelamapa", 1280, 720);
			allow_ver_dados();
		});
		
		function deny_ver_dados() {
			$("#button-ver-dados").attr("disabled", "disabled");
		}

		function allow_ver_dados() {
			$("#button-ver-dados").removeAttr("disabled");
		}
	});
</script>
