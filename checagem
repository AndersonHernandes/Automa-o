    $(document).ready(function() {
        $('#hist-comp-dentista').off('click').on('click', function() {
            pop_it_up_center('./../../admin/mapa_17_item_m1.php', 'infcomp', 1200, 800);
        });

        $('#procedimentos-vendidos-profissional').off('click').on('click', function() {
            pop_it_up_center('./../../admin/pop_procedimentos_vendidos_avaliacao.php', 'infcomp', 1200, 800);
        });

        $('.btn-ticket-medio').off('click').on('click', function() {
            let id_profissional = $(this).attr('id_profissional');
            let id_franquia = $(this).attr('id_franquia');
            pop_it_up_center('./../../admin/mapa_17_item_ticket_medio.php?user=' + id_profissional + '&franquia=' + id_franquia, 'infcomp', 1200, 800);
        });

        $('.btn-ultimos-30-dias').off('click').on('click', function() {
            let id_profissional = $(this).attr('id_profissional');
            let id_franquia = $(this).attr('id_franquia');
            let tipo_relatorio = $(this).attr('tipo_relatorio');
            pop_it_up_center('./../../admin/mapa_17_item_m2.php?user=' + id_profissional + '&franquia=' + id_franquia + '&tipo_relatorio=' + tipo_relatorio, 'infcomp', 1200, 800);
        });

        $('.btn-mes-corrente').off('click').on('click', function() {
            let id_profissional = $(this).attr('id_profissional');
            let id_franquia = $(this).attr('id_franquia');
            let tipo_relatorio = $(this).attr('tipo_relatorio');
            pop_it_up_center('./../../admin/mapa_17_item_m2.php?user=' + id_profissional + '&franquia=' + id_franquia + '&tipo_relatorio=' + tipo_relatorio, 'infcomp', 1200, 800);
        });

        $('#mes-corrente').off('click').on('click', function() {
            pop_it_up_center('./../../admin/mapa_17_item_m3.php', 'infcomp', 1200, 800);
        });
    });
</script>
