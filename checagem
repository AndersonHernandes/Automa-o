<script>
    /* +FACADE ------------------------------------------------------------------------------------------------------ */

    function _start_protocol_action() {
        event_save_manager();
        event_comment_add();
        event_comment_view();
        event_stars_view();
        event_change_status();
        event_change_status_final();
        event_start_stars();
        event_close_protocol();
        event_hist_video();
        event_magic_button();
        event_magic_button_add_email();
        event_send_password_to_email();
        event_renew_views();
        event_add_email();
        //---------------------
        event_external_link();
        event_erase_protocol();
        event_external_payment();
        send_whats();
        openSelectOrigemProtocolo();
        eventoSalvarOrigemProtocolo();
        add_picker('.sel-exp-gerentes');
    }

    /* -FACADE ------------------------------------------------------------------------------------------------------ */

    /* -FUNCTION ---------------------------------------------------------------------------------------------------- */
    function openSelectOrigemProtocolo () {
        _load('select-origem-protocolos', 'load-combo-origem-protocolos', {}, function() {
            add_picker('#tipos-origem-protocolos');
        })
    }

    function set_price_format() {
        $('.valor').priceFormat({
            clearPrefix: true,
            prefix: 'R$ ',
            centsSeparator: ',',
            thousandsSeparator: '.',
        });
    }

    function show_real_spend($this) {
        var $supervisor = currency_to_number($('#comissao-supervisor').val());
        var $gerente = currency_to_number($('#comissao-gerente').val());
        var $empresa = currency_to_number($('#comissao-empresa').val());
        var $representante = currency_to_number($('#comissao-representante').val());
        var $usuario = currency_to_number($('#comissao-usuario').val());

        var $total = $supervisor + $gerente + $empresa + $representante + $usuario;
        if (!isNaN($total)) {
            var $total = $total.toMoneyBRL();
            $('#total-comissao').html('');
            $('#total-comissao').hide();
            $('#total-comissao').fadeIn(function() {
                $('#total-comissao').html('Total Comissionado: R$ ' + $total);
                $this.removeAttr('disabled');
            });
            $('.btn-gravar-comissao').removeAttr('disabled');
        } else {
            swal('Atenção', 'É necessário adicionar os valores antes de continuar.');
            $this.removeAttr('disabled');
        }
    }

    /* -FUNCTION ---------------------------------------------------------------------------------------------------- */

    /* +EVENTOS ----------------------------------------------------------------------------------------------------- */
    function event_comment_add() {
        $('.button-add-comment').off('click');
        $('.button-add-comment').on('click', function() {
            var $this = $(this);

            $('.form-comentario').html('');
            $('.comment-view').html('');
            $('.button-add-comment').removeClass('comentario-atual');

            $this.addClass('comentario-atual');

            $('.form-final').html('');

            $this.attr('disabled', 'disabled').children().addClass('fa-spin').addClass('fa-spinner').removeClass('fa-comment');

            $this.parent().
            parent().
            find('.form-comentario').
            html('<div class=\'text-center\'><p></p><span class=\'fas fa-spinner fa-pulse fa-2x\'></span><h4>Carregando...</h4></div>');

            $this.parent().parent().find('.form-comentario').load('./load/exp_protocolo_comment.php', function() {
                event_comment_add_close();
                event_save_comment();

                $this.removeAttr('disabled').children().removeClass('fa-spin').removeClass('fa-spinner').addClass('fa-comment');

                $('#input-descricao').maxlength($maxLengthOptions);
                $('#input-descricao').focus();

            });
        });
    }

    function event_comment_add_close() {
        $('.fechar-formulario-comentario').off('click');
        $('.fechar-formulario-comentario').on('click', function() {
            var $this = $(this);
            $this.fadeOut('fast');

            var $comment = $this.parents('.form-comentario');

            $comment.slideUp('slow', function() {
                $comment.html('');
                $comment.show();
            });
        });
    }

    function event_save_comment() {
        $('#add-comment').click(function() {
            var $this = $(this);

            var $descricaox = $('#input-descricao').val();
            var $protocolo = $('.comentario-atual').val();
            var $protocolo_status = $('.comentario-atual').attr('protocolo_status');

            var $descricaox = $.trim($descricaox);

            if ($descricaox !== '' && $protocolo !== '') {
                if ($descricaox.length < 10) {
                    alert('Mínimo de 10 caracteres.');
                    $('#input-descricao').focus();
                    return false;
                }

                var $data = {
                    protocolox: $protocolo,
                    protocolo_statusx: $protocolo_status,
                    comentariox: $descricaox,
                };

                $.post('./acao/exp_add_comment.php', $data, function() {
                    $('.form-comentario').html('');
                    refresh_page_wo_warning();
                });
            } else {
                $('#input-descricao').focus();
            }
        });
    }

    function event_comment_view() {
        $('.button-comment-view').click(function() {
            var $this = $(this);
            var $protocol = $this.val();
            $this.siblings('.comment-view').
            html('<p></p><div class=\'text-center\'><span class=\'fas fa-spinner fa-pulse fa-2x\'></span><h4>Carregando...</h4></div>');

            $this.siblings('.comment-view').load('./load/exp_pro_vendas_comment_view.php', {
                protocolx: $protocol
            }, function() {
                event_comment_add_close();
                event_comment_close();
            });
        });
    }

