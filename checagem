<script>
    $(document).ready(function() {
        $.post('./ajax/refresh_session.php', {refresh: 1});
        setInterval(function() {
            $.post('./ajax/refresh_session.php', {refresh: 1});
        }, 300000);

        setInterval(function() {
            //Notificações
            if ($('.notif').find('li').hasClass('notif_green')) {
                setTimeout(function() {
                    $('.icone_notificacao_ativo').toggleClass('notif_green');
                    $('#cabecalho').css('border-bottom', '8px solid #8bc34a');
                    $('#rodape').css('border-top', '8px solid #8bc34a');
                }, 250);
                setTimeout(function() {
                    $('.icone_notificacao_ativo').toggleClass('notif_green');
                    $('#cabecalho').css('border-bottom', '8px solid #b71c1c');
                    $('#rodape').css('border-top', '8px solid #b71c1c');
                }, 750);
            } else {
                $('.icone_notificacao_ativo').removeClass('notif_green');
            }

            //Voz do Franqueado
            if ($('.notif').find('li').hasClass('notif_blue')) {
                setTimeout(function() {
                    $('.icone_voz_do_franqueado').toggleClass('notif_blue');
                    $('#cabecalho').css('border-bottom', '8px solid #42a5f5');
                    $('#rodape').css('border-top', '8px solid #42a5f5');
                }, 250);
                setTimeout(function() {
                    $('.icone_voz_do_franqueado').toggleClass('notif_blue');
                    $('#cabecalho').css('border-bottom', '8px solid #b71c1c');
                    $('#rodape').css('border-top', '8px solid #b71c1c');
                }, 750);
            } else {
                $('.icone_voz_do_franqueado').removeClass('notif_blue');
            }

            //Mensagem
            if ($('.notif').find('li').hasClass('notif_orange_mi')) {
                setTimeout(function() {
                    $('title').html('ATENÇÃO!');
                    $('.icone_mensageiro_ativo').toggleClass('notif_orange_mi');
                    $('#cabecalho').css('border-bottom', '8px solid #ff9800');
                    $('#rodape').css('border-top', '8px solid #ff9800');
                }, 250);
                setTimeout(function() {
                    $('title').html('NOVA MENSAGEM!');
                    $('.icone_mensageiro_ativo').toggleClass('notif_orange_mi');
                    $('#cabecalho').css('border-bottom', '8px solid #b71c1c');
                    $('#rodape').css('border-top', '8px solid #b71c1c');
                }, 750);
            } else {
                $('title').html('Sistema Odonto Excellence');
                $('.icone_mensageiro_ativo').removeClass('notif_orange');
            }

            //Vídeo
            if ($('#menu-v').find('li').hasClass('notificacao')) {
                setTimeout(function() {
                    $('.notificacao').toggleClass('video_ativo');
                    $('.span-icon').toggleClass('icon_ativo');
                }, 250);
                setTimeout(function() {
                    $('.notificacao').removeClass('video_ativo');
                    $('.span-icon').removeClass('icon_ativo');
                }, 750);
            } else {
                $('.notificacao').removeClass('video_ativo');
                $('.span-icon').removeClass('icon_ativo');
            }
        }, 1250);

        $('.scroll-top').click(function() {
            $('.scroll-top').attr('disabled', 'disabled');
            $('html, body').animate({scrollTop: 0}, 'slow', function() {
                $('.scroll-top').removeAttr('disabled');
            });
        });
    });
</script>
