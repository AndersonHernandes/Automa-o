<script>
		$(document).ready(function () {
			$(".comboFranquia").change(function() {
				let franquia_referencia = $(this).val();
				let retorno = '/sistemas/odonto/adm/mapa_trabalho_franqueado.php?interno=1'; 
				
				let $dados = {
					franquia_referencia, 
					retorno 
				};
				
				$.post('./acao/atualizar_sessao_franquia.php', $dados, function($ret) {				
					window.location.reload();	
				});
			});			
		});	
	</script>
