<script>
    $(document).ready(function() {
        $(".btn-inscrever-treinamento-1").off("click");
        $(".btn-inscrever-treinamento-1").on("click", function() {
            let fk_usuario = $('#usuario').val();
            let fk_franquia = $('#franquia').val();
            let id_treinamento = $('#treinamento_1').val();

            dados = {
                fk_usuario: fk_usuario,
                fk_franquia: fk_franquia,
                id_treinamento: id_treinamento
            };

            if (id_treinamento != "") {
                $.post("./acao/gravar_inscricao_usuario_treinamento.php", dados, function(ret) {
                    var ret = $.trim(ret);
                    if (confirm('Deseja Participar?')) {
                        if (ret == "OK") {
                            window.location.reload(true)
                        } else {
                            alert(ret);
                        }
                    }
                });
            }
        });

        $(".btn-inscrever-treinamento-2").off("click");
        $(".btn-inscrever-treinamento-2").on("click", function() {
            let fk_usuario = $('#usuario').val();
            let fk_franquia = $('#franquia').val();
            let id_treinamento = $('#treinamento_2').val();

            dados = {
                fk_usuario: fk_usuario,
                fk_franquia: fk_franquia,
                id_treinamento: id_treinamento
            };

            if (id_treinamento != "") {
                $.post("./acao/gravar_inscricao_usuario_treinamento.php", dados, function(ret) {
                    var ret = $.trim(ret);
                    if (confirm('Deseja Participar?')) {
                        if (ret == "OK") {
                            window.location.reload(true)
                        } else {
                            alert(ret);
                        }
                    }
                });
            }
        });
    });
</script> <hr style="border: thin solid #C00; max-width: 700px;">
