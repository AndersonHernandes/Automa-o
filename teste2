    function event_save_comment_final() {
        $('.add-comment-final').off('click');
        $('.add-comment-final').on('click', function() {
            var $this = $(this);

            var $protocolx = $this.attr('protocolx');
            var $to_statusx = $this.attr('to_statusx');
            var $commentx = $('.comment-final').val();

            if ($to_statusx == 6) {
                var $uf_venda = $('.sel-estados-br').val();
                var $cidade_venda = $('.cidade-destino').val();

                if ($uf_venda <= 0) {
                    swal('Aten√ß√£o', 'Selecione a UF na qual a unidade foi vendida.');
                    return;
                }

                if ($cidade_venda.length <= 3) {
                    $('.cidade-destino').focus();
                    return;
                }
            }

            if ($to_statusx == 4) {
                var $date_return = $('#data-escolher').val();
            }

            if ($commentx.length <= 5) {
                $('.comment-final').focus();
                return;
            }

            $this.attr('disabled', 'disabled');

            if ($to_statusx == 6) {
                save_comment_up($protocolx, $to_statusx, $uf_venda, $cidade_venda, $commentx);
            }

            if ($to_statusx == 5) {
                save_comment_down($protocolx, $to_statusx, $commentx);
            }

            if ($to_statusx == 4) {
                save_comment_wait($protocolx, $to_statusx, $date_return, $commentx);
            }

        });
    }

    function save_comment_up($protocolx, $to_statusx, $uf_venda, $cidade_venda, $commentx) {
        $.post('./acao/exp_change_protocol_status.php', {
            protocolx: $protocolx,
            change_status: $to_statusx,
            commentx: $commentx,
            uf_venda: $uf_venda,
            cidade_venda: $cidade_venda,
        }, function($return) {
            if ($.trim($return).length == 0) {
                $('.btn-change-status, .button-add-comment').attr('disabled', 'disabled');
                refresh_page_w_warning();
            } else {
                swal('Aten√ß√£o', $return);
            }
        });
    }

    function save_comment_down($protocolx, $to_statusx, $commentx) {
        $.post('./acao/exp_change_protocol_status.php', {
            protocolx: $protocolx,
            change_status: $to_statusx,
            commentx: $commentx,
        }, function($return) {
            if ($.trim($return).length == 0) {
                $('.btn-change-status, .button-add-comment').attr('disabled', 'disabled');
                refresh_page_w_warning();
            } else {
                swal('Aten√ß√£o', $return);
            }
        });
    }

    function save_comment_wait($protocolx, $to_statusx, $date_return, $commentx) {
        $.post('./acao/exp_change_protocol_status.php', {
            protocolx: $protocolx,
            change_status: $to_statusx,
            date_return: $date_return,
            commentx: $commentx,
        }, function($return) {
            if ($.trim($return).length == 0) {
                $('.btn-change-status, .button-add-comment').attr('disabled', 'disabled');
                refresh_page_w_warning();
            } else {
                swal('Aten√ß√£o', $return);
            }
        });
    }

    function event_close_form_final($panel_final_form) {
        $('.fechar-formulario').off();
        $('.fechar-formulario').on('click', function() {
            $panel_final_form.html('');
        });
    }

    function observer_resize() {
        add_picker('.select2');
        $('.select2').css('width', '100%');
        $(window).resize(function() {
            add_picker('.select2');
        });
    }

    function event_save_manager() {
        //observer_resize();
        $('.button-mudar-resp').off('click');
        $('.button-mudar-resp').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.attr('data-protocolo-id');
            var $respx = $('.sel-exp-gerentes').find('option:selected').val();
            var $origemProtocolo = $('#tipos-origem-protocolos').val();

            if ($respx <= 0) {
                $('.sel-exp-gerentes').focus();
                console.log('Can\'t handle operation.');
                return false;
            }

            $.post('./acao/exp_mudar_resp.php', {
                protocolx: $protocolx,
                respx: $respx,
                origemProtocolo: $origemProtocolo
            }, function($return) {
                if ($.trim($return).length == 0) {
                    $('.button-mudar-resp').attr('disabled', 'disabled');
                    refresh_page_w_warning();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });
        });
    }

    function eventoSalvarOrigemProtocolo() {
        $('.btn-mudar-origem').off('click').on('click', function() {
            $(".btn-mudar-origem").attr('disabled', 'disabled');
            let origemProtocoloId = $('#tipos-origem-protocolos').val();
            let protocoloId = $(this).attr('data-protocolo-id');

            if (origemProtocoloId == 0) {
                $('#tipos-origem-protocolos').focus();
                $(".btn-mudar-origem").removeAttr('disabled');
                return false;
            }

            salvarOrigemProtocolo(protocoloId, origemProtocoloId);
        })

    }

    function salvarOrigemProtocolo(protocoloId, origemProtocoloId) {
        _post('acao-mudar-origem-protocolo', {
            protocoloId,
            origemProtocoloId
        }, function(result) {
            if (result.trim() == 'OK') {
                swal('Origem alterada com sucesso!', {
                    icon: "success"
                }).then((close) => {
                    refresh_page_w_warning();
                });
            } else {
                swal('N√£o foi poss√≠vel alterar, favor tentar novamente!', {
                    icon: "error"
                }).then((close) => {
                    $(".btn-mudar-origem").removeAttr('disabled');
                });
            }
        })
    }

    function event_start_stars() {
        if ($('.stars').length >= 1) {
            $('.stars').addClass('cursor-pointer');
            $('.stars').hover(function() {
                $(this).children().addClass('fa-spin').addClass('text-color-pomegranate').removeClass('far').addClass('fas');
                $(this).prevAll().children().addClass('fa-spin').addClass('text-color-pomegranate').removeClass('far').addClass('fas');
            }, function() {
                $(this).children().removeClass('fa-spin').removeClass('text-color-pomegranate').addClass('far').removeClass('fas');
                $(this).prevAll().children().removeClass('fa-spin').removeClass('text-color-pomegranate').addClass('far').removeClass('fas');
            });

            $('.stars').click(function() {
                var $this = $(this);
                var protocolox = $(this).attr('protocolox');
                var starx = $(this).attr('star');

                $.post('./acao/gravar_estrela_negociacao.php', {
                    protocolox: protocolox,
                    starx: starx
                }, function(data) {
                    if ($.trim(data.toString()) === '') {
                        var $stars_place = $this.closest('.estrelas');
                        $stars_place.html('<span class=\'text-success\'>Registrando...<span><p></p>');
                        setTimeout(function() {
                            refresh_page_wo_warning();
                        }, 2000);
                    } else {
                        swal('Aten√ß√£o', data);
                    }
                });
            });
        }
    }

    function event_close_protocol() {
        $('.button-close-protocol').off('click');
        $('.button-close-protocol').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.val();

            $('.fechar-formulario-end-protocol').trigger('click');

            $('.form-close-protocol').html('');
            $('.button-close-protocol').removeClass('comentario-atual');

            $this.addClass('comentario-atual');

            $this.attr('disabled', 'disabled').children().addClass('fa-spin').addClass('fa-spinner').removeClass('fa-comment');

            $this.parent().
            parent().
            find('.form-close-protocol').
            html('<div class=\'text-center\'><br><span class=\'fas fa-spinner fa-pulse fa-2x\'></span><h4>Carregando...</h4></div>');

            $this.parent().parent().find('.form-close-protocol').load('./load/exp_end_protocol.php', {
                protocolx: $protocolx
            }, function() {
                $('.btn-gravar-comissao').attr('disabled', 'disabled');

                event_end_protocol_add_close();
                set_price_format();
                event_on_calc();
                event_save_end_protocol($protocolx);

                $this.removeAttr('disabled').children().removeClass('fa-spin').removeClass('fa-spinner').addClass('fa-checked');

                $('#input-descricao').maxlength($maxLengthOptions);
                $('#input-descricao').focus();

            });
        });
    }

    function event_end_protocol_add_close() {
        $('.fechar-formulario-end-protocol').off('click');
        $('.fechar-formulario-end-protocol').on('click', function() {
            var $this = $(this);
            $this.fadeOut('fast');

            var $comment = $this.parents('.form-close-protocol');

            $comment.slideUp('slow', function() {
                $comment.html('');
                $comment.show();
            });
        });
    }

    function event_save_end_protocol(protocolx) {
        $('.btn-gravar-comissao').off('click');
        $('.btn-gravar-comissao').on('click', function() {
            var $this = $(this);

            var $protocolx = protocolx;
            var $to_status = 8;

            var $supervisor = $('#comissao-supervisor').val();
            var $gerente = $('#comissao-gerente').val();
            var $empresa = $('#comissao-empresa').val();
            var $representante = $('#comissao-representante').val();
            var $usuario = $('#comissao-usuario').val();

            if (false /* JUMP FOR NOW */ ) {

                if ($supervisor.length <= 3) {
                    swal('Aten√ß√£o', 'Por favor, adicione o valor da Comiss√£o do Supervisor.');
                    return false;
                }
                if ($gerente.length <= 3) {
                    swal('Aten√ß√£o', 'Por favor, adicione o valor da Comiss√£o do Gerente.');
                    return false;
                }
                if ($empresa.length <= 3) {
                    swal('Aten√ß√£o', 'Por favor, adicione o valor da Comiss√£o da Empresa.');
                    return false;
                }
                if ($representante.length <= 3) {
                    swal('Aten√ß√£o', 'Por favor, adicione o valor da Comiss√£o do Representante.');
                    return false;
                }
            }

            $.post('./acao/exp_change_protocol_status.php', {
                protocolx: $protocolx,
                change_status: $to_status,
                supervisorx: $supervisor,
                gerentex: $gerente,
                empresax: $empresa,
                representantex: $representante,
                usuariox_exp: $usuario,
            }, function($return) {
                if ($.trim($return).length == 0) {
                    $('.btn-change-status, .button-add-comment').attr('disabled', 'disabled');
                    refresh_page_w_warning();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });

            console.log($protocolx);
        });
    }

    function event_on_calc() {
        $('.button-calcular').off('click');
        $('.button-calcular').on('click', function() {
            var $this = $(this);
            $this.attr('disabled', 'disabled');
            $('.btn-gravar-comissao').attr('disabled', 'disabled');
            show_real_spend($this);
        });
    }

    function event_external_link() {
        $('.button-external-link').off('click');
        $('.button-external-link').on('click', function() {
            var $this = $(this);
            var $value = $this.val();

            $this.preventDefault;
            $this.attr('disabled', 'disabled');
            $.post('./acao/set_temp_var.php', {
                atributox: 'sistema_controle_exp_work_protocol',
                valorx: $value
            }, function(returnx) {
                pop_it_up_center('./pop_exp_external_prot.php', 'extprot', 1280, 720);
                $this.removeClass('btn-primary').addClass('btn-default');
                setTimeout(function() {
                    $this.removeAttr('disabled');
                }, 2000);
            });
        });
    }

    function event_erase_protocol() {
        $('.button-erase-protocol').off('click');
        $('.button-erase-protocol').on('click', function() {
            var $this = $(this);
            var $value = $this.val();

            $this.preventDefault;
            $this.attr('disabled', 'disabled');

            swal('Realmente excluir, o processo irrevers√≠vel?', {
                buttons: {
                    cancel: 'Cancelar',
                    deletar: {
                        text: 'Deletar',
                        value: true,
                    },
                },
            }).then((value) => {
                switch (value) {
                    case 'Cancelar':
                        break;

                    case true:
                        $this.closest('tr').remove();
                        $.post('./acao/exp_delete_protocol.php', {
                            valorx: $value
                        }, function(returnx) {
                            call_protocol();
                            swal('Apagado!', 'Protocolo apagado.', 'success');
                            setTimeout(function() {
                                $this.removeAttr('disabled');
                            }, 2000);
                        });
                        break;
                }
            });

            $this.removeAttr('disabled');
        });
    }

    function event_external_payment() {
        $('.button-pagar').off('click');
        $('.button-pagar').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.val();
            var $to_status = 111; //AUTHORIZE PAYMENT
            var $line = $this.parent().parent();

            $this.attr('disabled', 'disabled');
            $this.attr('disabled', 'disabled');
            $this.attr('disabled', 'disabled');

            $line.hide();
            $.post('./acao/exp_change_protocol_status_master.php', {
                protocolx: $protocolx,
                change_status: $to_status,
            }, function($return) {
                if ($.trim($return).length == 0) {
                    reload_protocolos();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });
        });

        $('.button-estornar').off('click');
        $('.button-estornar').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.val();
            var $to_status = 222; //REVOKE PAYMENT
            var $line = $this.parent().parent();

            $this.attr('disabled', 'disabled');
            $this.attr('disabled', 'disabled');
            $this.attr('disabled', 'disabled');

            $line.hide();
            $.post('./acao/exp_change_protocol_status_master.php', {
                protocolx: $protocolx,
                change_status: $to_status,
            }, function($return) {
                if ($.trim($return).length == 0) {
                    reload_protocolos();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });
        });
    }

    function event_add_email() {
        $('#add-email-prot-vazio').off('keypress').on('keypress', function(key) {
            if (key.which === 13) {
                let _this = $(this);
                let protx = $('#protocolx').attr('protocolx');
                let emailx = $('#add-email-prot-vazio').val();

                if (emailx.length >= 1) {
                    if (!validate_email(emailx)) {
                        swal('Email incorreto.');
                        return false;
                    }
                } else {
                    swal('√â preciso adicionar um email.');
                    return false;
                }

                if (protx <= 1) {
                    return false;
                }

                $.post('./acao/exp_add_email_prot.php', {
                    protx: protx,
                    emailx: emailx
                }, function() {
                    refresh_page_w_warning();
                });
            }
        });
    }

    function event_renew_views() {
        $('.button-hist-reset-view').off('click');
        $('.button-hist-reset-view').on('click', function() {
            var $this = $(this);
            var $userx = $this.attr('userx');

            $this.attr('disabled', 'disabled');
            $this.text('Renovando...');
            $.post('acao/exp_renew_views.php', {
                userx: $userx
            }, function($return) {
                if ($.trim($return).length == 0) {
                    refresh_page_wo_warning();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });

            if (false) {

                swal({
                    title: 'Tem certeza?',
                    text: 'N√£o √© poss√≠vel desfazer a opera√ß√£o!',
                    type: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#dd6b55',
                    cancelButtonText: 'Cancelar',
                    confirmButtonText: 'Resetar',
                    closeOnConfirm: false,
                }, function() {
                    $this.attr('disabled', 'disabled');
                    $this.text('Renovando...');
                    $.post('acao/exp_renew_views.php', {
                        userx: $userx
                    }, function($return) {
                        if ($.trim($return).length == 0) {
                            refresh_page_wo_warning();
                        } else {
                            swal('Aten√ß√£o', $return);
                        }
                    });
                });
            }
        });
    }

    function send_whats() {
        $('.whatsx').each(function(index, obj) {
            whats_to_desktop(index, obj);
        });
    }

    function whats_to_desktop(index, obj) {
        let $this = $(obj);

        let uri = 'https://wa.me/';
        let phone = $this.attr('phone');
        let msg = $this.attr('msg');

        let url = uri + phone + '?text=' + msg;

        $this.attr('href', url);
    }

