<script>
    $(document).ready(function () {
        var pesquisa;
        var alternativa;
        var justificativa;
        carregarPesquisa();
        function carregarPesquisa() {
            $("#painel_pesquisa_franqueado").load("painel_pesquisa_franqueado.php", {id_tipo_usuario: 10}, function () {});
            
        }

        $("body").on("click", ".alternativa", function () {
            pesquisa = $(this).parent(".pesquisa").attr("id");
            alternativa = $(this).attr("id");
            justificativa = $(this).attr("justificativa");
            if (justificativa == "1") {
                $("#justificativa_alternativa").show();
            } else {
                $("#justificativa_alternativa").hide();
                $("#justificativa_text").val("");
            }
            $(".alternativa").removeClass("selected");
            $(this).addClass("selected");
        });

        $("body").on("click", "#responder_pesquisa", function () {
            if (!alternativa) {
                alert("Favor escolher uma alternativa");
            } else {
                var motivo_justificativa = $("#justificativa_text").val().trim();
                if (justificativa == "0" || (justificativa == "1" && motivo_justificativa.length >= 10)) {
                    $(this).attr("disabled", "true");
                    $.ajax({
                        type: 'POST',
                        url: 'gravar_resposta_pesquisa_franqueado.php',
                        data: {fk_pesquisa: pesquisa, fk_alternativa: alternativa, justificativa: motivo_justificativa},
                        success: function (data) {
                            $("#cssmenu").show();
                            $(".button-menu").show();
                        //    alert("Agradecemos a sua participação!");
                            carregarPesquisa();
                            alternativa = null;
                        }
                    });
                } else {
                    alert("Favor informar o motivo da escolha");
                }

            }
        });
    });
</script>
