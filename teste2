     * EFETIVAR OU DECLINAR PROTOCOLO
     */
    function event_change_status_final() {
        $('.btn-change-status-final').off('click');
        $('.btn-change-status-final').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.val();
            var $to_status = $this.attr('to_status');

            var $panelx = $('.protocol-' + $protocolx);

            var $panel_heading = $panelx.children('.card-header');
            var $panel_body = $panelx.children('.card-body');
            var $panel_footer = $panelx.children('.card-footer');

            var $panel_final_form = $panel_footer.children('.form-final');
            // cadastrar_franquia();
            $('.form-final').html('');
            $panel_final_form.load('./load/exp_form_prot_final.php', {
                    protocolx: $protocolx,
                    to_statusx: $to_status,
                },
                function() {
                    add_picker('.select');
                    //observer_resize();
                    event_save_comment_final();
                    event_close_form_final($panel_final_form);
                });
        });
    }
    /*function cadastrar_franquia() {
