<script>
            function fechar() {
                $('.meu_modal').colorbox.close();
            }

            $(document).ready(function() {
                $('.meu_modal').colorbox({iframe: true, width: '700px', height: '430px', overlayClose: false});
                $('.meu_modal_003_2').colorbox({iframe: true, width: '800px', height: '400px', overlayClose: false});
                $('.meu_modal_003_3').colorbox({iframe: true, width: '850px', height: '400px', overlayClose: false});
                $('.meu_modal_003_5').colorbox({iframe: true, width: '850px', height: '500px', overlayClose: false});
                $('.meu_modal_015').colorbox({iframe: true, width: '700px', height: '450px', overlayClose: false});
                $('.meu_modal_017').colorbox({iframe: true, width: '700px', height: '450px', overlayClose: false});
                $('.meu_modal_001').colorbox({iframe: true, width: '700px', height: '450px', overlayClose: false});
                $('.meu_modal_016').colorbox({iframe: true, width: '800px', height: '450px', overlayClose: false});
                $('.meu_modal_04').colorbox({iframe: true, width: '450', height: '300', overlayClose: false});
                //$(".inline").colorbox({inline:true, width:"400"});
            });

            function abre_proc(qq_trat, qq_franquia, qq_prof, qq_data, qq_id) {
                // abre janela do cadastro do cliente
                $.colorbox({
                    iframe: true,
                    width: '850',
                    height: '500',
                    overlayClose: true,
                    href: 'mapa_trabalho_franqueado_inc003_modal_5b.php?tratamento=' + qq_trat + '&franquia=' + qq_franquia + '&prof=' + qq_prof + '&data=' + qq_data,
                    onClosed: function() {
                        re_abre(qq_trat, qq_franquia, qq_prof, qq_data, qq_id);
                    }, // quando fecha tem que abrir novamente a janela da cobranca
                });
            }

            function re_abre(qq_trat, qq_franquia, qq_prof, qq_data, qq_id) {
                //alert(qq_cliente);
                var j_href = 'mapa_trabalho_franqueado_inc003_modal_5.php?franquia=' + qq_franquia + '&prof=' + qq_prof + '&data=' + qq_data + '&linha_marcada=' + qq_id;
                // abrindo a janela de cobranca
                $.colorbox({
                    iframe: true, width: '850', height: '500', overlayClose: false, //closeButton: false,
                    escKey: false, href: j_href,
                });
            }

            function abre_negativados(qq_franquia) {
                //alert(qq_cliente);
                var j_href = 'mt_box_18c.php?franquia=' + qq_franquia;
                // abrindo a janela de cobranca
                $.colorbox({
                    iframe: true, width: '700', height: '450', overlayClose: false, //closeButton: false,
                    escKey: false, href: j_href,
                });
            }

        </script>

