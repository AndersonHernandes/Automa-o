    $('#system').appendTo($('#info-memoria'));

    function msg_notify_by_system_lite($title, $msg, $tag = 'normal') {
        var options = {
            body: $msg, lang: 'pt-BR', tag: $tag,
        };

        var n = new Notification($title, options);
    }

    function blinkMenuIndicacao() {
        setInterval(function() {
            $('#indicacao_cliente').toggleClass('orange');
        }, 750);
    }
</script>
