<script>
    $(document).ready(function() {
        $(".btn-escolha-menu").click(function() {
            let escolha_menu = $(this).attr("escolha_menu");

            let dados = {
                escolha_menu: escolha_menu
            };

            if (escolha_menu != "") {
                $.post("./acao/gravar_escolha_menu.php", dados, function(ret) {
                    if (ret == "OK") {
                        window.location = './home.php';
                    } else {
                        alert(ret);
                    }
                });
            }
        });
    });
</script>
