    /**
     * ADICIONAR EMAIL PARA - CRIAR O USU√ÅRIO E ENVIAR O V√çDEO
     * function event_magic_button()
     */
    function event_magic_button_add_email() {
        $('.button-create-user-add-email').off('click');
        $('.button-create-user-add-email').on('click', function() {
            let $this = $(this);
            let $protocolx = $this.val();

            $this.attr('disabled', 'disabled');

            $this.attr('disabled', 'disabled').children().addClass('fa-spin').addClass('fa-spinner').removeClass('fa-comment');

            $this.parent().
            parent().
            find('.form-comentario').
            html('<div class=\'text-center\'><p></p><span class=\'fas fa-spinner fa-pulse fa-2x\'></span><h4>Carregando...</h4></div>');

            $this.parent().parent().find('.form-comentario').load('./load/exp_protocolo_add_email.php', {
                protocolox: $protocolx
            }, function() {
                event_comment_add_close();
                event_save_comment();

                $this.removeAttr('disabled').children().removeClass('fa-spin').removeClass('fa-spinner').addClass('fa-comment');

                $('#input-descricao').maxlength($maxLengthOptions);
                $('#input-descricao').focus();

            });
        });
    }

    function event_change_status($status) {
        $('.btn-change-status').off('click');
        $('.btn-change-status').on('click', function() {
            var $this = $(this);
            var $protocolx = $this.val();
            var $to_status = $this.attr('to_status');
            var $tipo_venda = false;
            if ($to_status == 8) {
                $tipo_venda = $('input[name=\'tipo_venda_' + $protocolx + '\']:checked').val();
                if ($tipo_venda == undefined || !$tipo_venda) {
                    swal('Aten√ß√£o', 'Selecione um tipo de venda.');
                    return false;
                }
                console.log($tipo_venda);
            }

            $.post('./acao/exp_change_protocol_status.php', {
                protocolx: $protocolx,
                change_status: $to_status,
            }, function($return) {
                if ($.trim($return).length == 0) {
                    $('.btn-change-status, .button-add-comment').attr('disabled', 'disabled');
                    refresh_page_w_warning();
                } else {
                    swal('Aten√ß√£o', $return);
                }
            });
        });
    }

