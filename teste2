<script>
        $(document).ready(function() {
            $(".btn-inscrever-ferramenta-termo").off("click");
            $(".btn-inscrever-ferramenta-termo").on("click", function() {
                let preenchido = 0;
                let franquias = new Array();
                $("input[name='franquiasEscolhaFerramenta[]']:checked").each(function() {
                    franquias.push($(this).val());
                    if (franquias) {
                        preenchido = 1;
                    }
                });

                if (preenchido == 1) {
                    if (franquias) {
                        dados = {
                            franquias: franquias
                        };

                        if (confirm('Deseja gravar?')) {
                            $.post("./acao/gravar_escolha_franquia_termo_conclusao_procedimento.php", dados, function(ret) {
                                var ret = $.trim(ret);
                                if (ret == "OK") {
                                    window.location.reload(true);
                                } else {
                                    alert(ret);
                                }
                            });
                        }
                    }
                } else {
                    alert("Escolha uma Franquia antes de gravar");
                }
            });
        });
    </script>
