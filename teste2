<script>
    $(document).ready(function() {
        function pop_it_up_center(page_URL, title, w, h) {
            let left = (screen.width / 2) - (w / 2);
            let top = (screen.height / 2) - (h / 2);
            let popup = window.open(page_URL, title, 'width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
            if (window.focus) {
                popup.focus();
            }
        }

        $(".btn-ver-top-ranking").on("click", function() {
            let categoria = $(this).attr('categoria');
            let pais = $('#pais').val();
            let mes = $('#mes').val();
            let ano = $('#ano').val();

            pop_it_up_center('./top_ranking_mostrar.php?pais=' + pais + '&mes=' + mes + '&ano=' + ano + '&categoria=' + categoria, 'Top Ranking', 1200, 800);
        });
    });
</script> <hr style="border: thin solid #C00; max-width: 700px;">
